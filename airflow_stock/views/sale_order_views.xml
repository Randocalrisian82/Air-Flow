<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_order_form_inherit_airflow_attn" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.airflow.attn</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='ship_date']" position="before">
                <field name="ship_attn"/>
            </xpath>
        </field>
    </record>

</odoo>
